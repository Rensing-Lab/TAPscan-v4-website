* {
    margin: 0;
    padding: 0;
}
html{
    height: 100%
}

body{
    background: #004600; /* For browsers that do not support gradients */
    background: -webkit-linear-gradient(#004600, black, #004600); /* For Safari 5.1 to 6.0 */
    background: -o-linear-gradient(#004600, black, #004600); /* For Opera 11.1 to 12.0 */
    background: -moz-linear-gradient(#004600, black, #004600); /* For Firefox 3.6 to 15 */
    background: linear-gradient(#004600, black, #004600); /* Standard syntax */
    background-attachment:fixed;
    margin-left: 15px;
    margin-right: 15px;
    height: 90%;
    color:#004600;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 0.8em;
}

.body{
    max-width: 970px;
    background-color: #EBFFDA;
    margin: 0 auto;
    min-height: 85%;
    box-shadow: 0px 20px 20px black;
    -moz-box-shadow: 0px 0px 20px black;
    -webkit-box-shadow: 0px 0px 20px black;
}

.top{
    position: relative;
    max-width: 970px;
    background-color: rgba(0,0,0,0);
    margin: 0 auto;
}

.content{
    padding: 15px;
}

#impressum{
    font-family: "Courier New", Courier, monospace;
    font-size: 17px;
    position: fixed;
    bottom: 0;
    background-color: #003300;
    right: 10px;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    -moz-border-top-left-radius: 10px;
    -moz-border-top-right-radius: 10px;
    -webkit-border-top-left-radius: 10px;
    -webkit-border-top-right-radius: 10px;
    padding: 10px 15px 5px 15px;
    border: solid #000000;
    border-width: 2px 2px 0 2px;
    color: #96C382;
}

#impressum:hover{
    background-color: #004600;
    color: white;
}

#impressum a{
    position:absolute;
    width:100%;
    height:100%;
    top:0;
    left:0;
    z-index:10;
    color: #004600;
    display: block;
    text-decoration: underline;
    background-color:white;
    opacity: 0;
    filter: alpha(opacity=1);
}

hr{
    margin-top: 15px;
    margin-bottom: 15px;
}

#logo{
    display: block;
    margin-left: auto;
    margin-right: auto;
    margin-bottom: 5px;
    margin-top: 3px;
    max-width: 100%;
    max-height: 150px;
}

#plogo{
    max-height: 30px;
}

#overlay_logo{
    position: absolute;
    bottom: -8px;
    right: 0px;
}

.navigation {
    list-style-type: none;
    margin: 0;
    padding: 0;
    overflow: hidden;
    border: 1px solid #003300;
    background-color: #004600;
}

.navigation li {
    float: left;
    font-size: 17px;
    font-family: "Courier New", Courier, monospace;
}

.navigation li a {
    display: block;
    color: #96C382;
    text-align: center;
    padding: 10px 16px;
    text-decoration: underline;
}

.navigation li a:hover:not(.active) {
    background-color: #003300;
}

.navigation li a.active {
    color: #96C382;
    background-color: #003300;
}

h1{
    font-family: Arial, Helvetica, sans-serif;
    font-size: 19px;
    margin-bottom: 10px;
    color: #004600;
}

h2{
